<h1>Please Log in</h1>
</br>
<%= form_tag sessions_path do %>
  <div class="field">
    <%= label_tag :email %>
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <div class="actions"><%= submit_tag "Log in" %></div>
</br>
</br>
  
    <%= link_to "Not a Member? Sign Up!", signup_path, class:"btn btn-large btn-primary" %>
<% end %>



